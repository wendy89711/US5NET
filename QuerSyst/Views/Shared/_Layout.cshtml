<!DOCTYPE html>
<html>


<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - 資訊系統線上諮詢服務系統</title>
    <!-- css -->
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    <!-- js -->
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)

</head>


<body>
    <div class="navbar navbar-expand-lg fixed-top navbar-dark bg-primary">
        <div class="container">
            <a class="navbar-brand" href="@Url.Action("Index", "Home")">
                <svg width="1em" height="1em" viewBox="0 0 16 16" class="bi bi-house-door-fill" fill="currentColor" xmlns="http://www.w3.org/2000/svg">
                    <path d="M6.5 10.995V14.5a.5.5 0 0 1-.5.5H2a.5.5 0 0 1-.5-.5v-7a.5.5 0 0 1 .146-.354l6-6a.5.5 0 0 1 .708 0l6 6a.5.5 0 0 1 .146.354v7a.5.5 0 0 1-.5.5h-4a.5.5 0 0 1-.5-.5V11c0-.25-.25-.5-.5-.5H7c-.25 0-.5.25-.5.495z" />
                    <path fill-rule="evenodd" d="M13 2.5V6l-2-2V2.5a.5.5 0 0 1 .5-.5h1a.5.5 0 0 1 .5.5z" />
                </svg>
            </a>
            <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarColor01" aria-controls="navbarColor01" aria-expanded="false" aria-label="Toggle navigation">
                <span class="navbar-toggler-icon"></span>
            </button>

            <div class="collapse navbar-collapse" id="navbarColor01">
                <ul class="navbar-nav mr-auto">
                    <li class="nav-item">
                        <a class="nav-link" href="@Url.Action("Quer", "Quer")">諮詢問題</a>
                    </li>
                    @*<li class="nav-item">
                        <a class="nav-link" href="#">查詢案件進度</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="#">檢視回覆內容</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="#">查詢Q&A</a>
                    </li>*@
                    @*<li class="nav-item">
                        @Html.ActionLink("連結", "About", "Home", new { @class = "nav-link" })
                    </li>*@
                    <li class="nav-item dropdown">
                        <a class="nav-link dropdown-toggle show" data-toggle="dropdown" href="#" id="manager" aria-expanded="false">
                            後台管理
                            <span class="caret"></span>
                        </a>
                        <div class="dropdown-menu" aria-labelledby="manager">
                            <a class="dropdown-item" href="@Url.Action("Index", "Ansr")">回覆問題</a>
                            @*<a class="dropdown-item" href="">查詢統計資料</a>*@
                            @*<a class="dropdown-item" href="@Url.Action("Index", "SystClas")">@Html.ActionLink("問題分類維護", "Index", "SystClas")</a>*@
                            <a class="dropdown-item" href="@Url.Action("Index", "SystClas")">問題分類維護</a>

                            @*@Html.ActionLink("問題分類維護", "Index", "SystClas", new { @class = "dropdown-item"})*@
                            <a class="dropdown-item" href="@Url.Action("Index", "SystQAdb")">Q&A維護</a>
                        </div>
                    </li>
                </ul>
                <!--
                    <form class="form-inline my-2 my-lg-0">
                    <input class="form-control mr-sm-2" type="text" placeholder="Search">
                    <button class="btn btn-secondary my-2 my-sm-0" type="submit">Search</button>
                    </form>
                -->
            </div>
        </div>
    </div>
    <div class="container body-content">
        <br />
        @RenderBody()
        <hr />
        <footer>
            <p>&copy; @DateTime.Now.Year - 資訊系統線上諮詢服務系統</p>
        </footer>
    </div>


</body>
</html>
